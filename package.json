{
  "name": "Overmind",
  "version": "0.5.2",
  "description": "Overmind Screeps AI",
  "author": "Ben Bartlett",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencbartlett/Overmind.git"
  },
  "homepage": "https://github.com/bencbartlett/Overmind#readme",
  "bugs": {
    "url": "https://github.com/bencbartlett/Overmind/issues"
  },
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint \"src/**/*.ts\"",
    "clean": "rm -rf tsc-out && rm -rf dist",
    "compile": "rollup -c",
    "push-main": "rollup -c --environment DEST:main",
    "push-plus": "rollup -c --environment DEST:screepsplus",
    "push-sim": "rollup -c --environment DEST:sim",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-plus": "rollup -cw --environment DEST:screepsplus",
    "watch-sim": "rollup -cw --environment DEST:sim",
    "test": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/lodash": "3.10.1",
    "@types/screeps": "2.5.2",
    "@types/screeps-profiler": "^1.2.2",
    "rollup": "0.62.0",
    "rollup-plugin-commonjs": "8.4.1",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-screeps": "0.1.2",
    "rollup-plugin-typescript2": "0.16.1",
    "screeps-profiler": "^2.0.1",
    "tslint": "5.11.0",
    "typedoc": "^0.14.2",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "columnify": "1.5.4",
    "source-map": "0.7.3"
  }
}
